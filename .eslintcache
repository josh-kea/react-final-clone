n    constructor(props) {\r\n        super(props);\r\n    }\r\n\r\n    state = {\r\n        email: \"\",\r\n        password: \"\"\r\n    };\r\n\r\n\r\n    verifyEmail = (event) => {\r\n        fetch(`http://localhost:4000/verifyEmail/${this.props.match.params.verifyString}`, {\r\n            method:'GET',\r\n            headers: {\r\n                'Content-Type': 'application/json'\r\n            },\r\n        })\r\n        .then(response => {\r\n            if (response.ok) {\r\n                alert(\"Email verified!\")\r\n            } else {\r\n                alert(\"Sorry, but that verification link is invalid!\")\r\n            }\r\n            return response\r\n        })\r\n        .then(\r\n                this.props.history.push('/')\r\n        )\r\n        .catch(error => console.log('ERROR'))\r\n    }\r\n\r\n    componentDidMount() {\r\n        this.verifyEmail();\r\n    }\r\n\r\n    render() {\r\n        return (\r\n            <div className=\"container\">\r\n                \r\n            <p>Verifying email...</p>\r\n            \r\n            </div>\r\n        );\r\n    }\r\n}\r\n\r\nexport default withRouter(Verify);\r\n","C:\\Users\\Joshua\\Documents\\04-Feb React 2.0\\01 React Project - Final\\src\\SignupPage.js",[],"C:\\Users\\Joshua\\Documents\\04-Feb React 2.0\\01 React Project - Final\\src\\LoginPage.js",["109"],"\r\nimport './App.css';\r\nimport React, { useState, useEffect } from 'react';\r\nimport { Link, withRouter } from 'react-router-dom';\r\nimport { verifyToken, logout, getUser } from './helpers.js';\r\nimport './LoginPage.css'\r\n\r\nconst LoginPage = (props) => {\r\n\r\n  const [state, setState] = useState( {\r\n    email: '',\r\n    password: '',\r\n    message: ''\r\n  })\r\n\r\n  const handleFormLoginUser = (event) => {\r\n      const { email, password } = state\r\n\r\n        fetch(`http://localhost:4000/users/login`, {\r\n            method:'POST',\r\n            headers: {\r\n                'Content-Type': 'application/json'\r\n            },\r\n            body: JSON.stringify({\r\n                email: email,\r\n                password: password\r\n            })\r\n        })\r\n        .then(response => {\r\n            return response.json()\r\n        })\r\n        .then(data => {\r\n            if (data.authToken) {\r\n                sessionStorage.setItem('token', JSON.stringify(data.authToken))\r\n                props.history.push('/')\r\n            } else {\r\n                setState({ message: data.message})\r\n            }\r\n\r\n        })\r\n        .catch(error => console.log('ERROR'))\r\n\r\n    event.preventDefault();\r\n    \r\n}\r\n\r\n\r\n  function handleChange(name) {\r\n    return function(event) {\r\n      setState({ ...state, [name]: event.target.value })\r\n    };\r\n  };\r\n\r\n  useEffect(() => {\r\n    console.log(\"UseEffect Mounted\")\r\n    if (verifyToken()) props.history.push('/')       \r\n  }, [])\r\n\r\n  return (\r\n    \r\n    <div className=\"container\">\r\n      { !verifyToken() && (\r\n        <div className=\"edit-user-modal\">\r\n                <div className=\"modal-form-wrapper\">\r\n                    <div className=\"modal-form-header\">\r\n                        <p>Login</p>\r\n                        <Link to=\"/signup\" className=\"sign-in-btn\">Don't have an account? Sign up.</Link>\r\n                    </div>\r\n                    <form className=\"modal-form\" onSubmit={handleFormLoginUser} id=\"LoginForm\">\r\n                        <div className=\"form-row\">\r\n                            <div className=\"row-wrapper\">\r\n                                <p>Email</p>\r\n                                <input value={state.email} onChange={handleChange('email')} type=\"email\" placeholder=\"Email\" required/>\r\n                            </div>\r\n                        </div>\r\n                        <div className=\"form-row\">\r\n                            <div className=\"row-wrapper\">\r\n                                <p>Password</p>\r\n                                <input value={state.password} onChange={handleChange('password')} type=\"password\" placeholder=\"Your password\" required/>\r\n                            </div>\r\n                        </div>\r\n                        <div className=\"form-row\">\r\n                            \r\n                            <div className=\"row-wrapper  row-end\">\r\n                                <div className=\"signup-message\">{state.message}</div>\r\n                                <div className=\"form-btn\" onClick={() => props.history.push('/')}>Cancel</div>\r\n                                <button className=\"form-btn\">Log In</button>\r\n                            </div>\r\n                        </div>\r\n\r\n                    </form>\r\n                </div>\r\n            </div>\r\n\r\n      )}\r\n\r\n      { verifyToken() && (\r\n        <div>Hello <span style={{color:\"red\"}}>{getUser()}</span>! You are logged in already. <span onClick={() => logout(() => props.history.push('/'))}> Logout</span></div>\r\n      )}\r\n\r\n    </div>\r\n  );\r\n}\r\n\r\nexport default withRouter(LoginPage);\r\n",["110","111"],"C:\\Users\\Joshua\\Documents\\04-Feb React 2.0\\01 React Project - Final\\src\\HomePage.js",["112","113","114","115","116","117","118","119","120"],"C:\\Users\\Joshua\\Documents\\04-Feb React 2.0\\01 React Project - Final\\src\\AdminPages\\AdminSingleUser.js",[],"C:\\Users\\Joshua\\Documents\\04-Feb React 2.0\\01 React Project - Final\\src\\NavbarMain.jsx",[],"C:\\Users\\Joshua\\Documents\\04-Feb React 2.0\\01 React Project - Final\\src\\HomeAllProducts.jsx",["121","122"],"C:\\Users\\Joshua\\Documents\\04-Feb React 2.0\\01 React Project - Final\\src\\ViewProductButton.jsx",["123","124"],"C:\\Users\\Joshua\\Documents\\04-Feb React 2.0\\01 React Project - Final\\src\\AdminPages\\components\\users\\SingleUser.jsx",["125","126","127","128","129"],"C:\\Users\\Joshua\\Documents\\04-Fe